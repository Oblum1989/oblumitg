{
  "flex-layout.row#banner__home-mobile": {
    "props": {
      "horizontalAlign": "center",
      "verticalAlign": "center",
      "preventHorizontalStretch": false,
      "preventVerticalStretch": false,
      "fullWidth": true
    },
    "children": [
      "flex-layout.col#text__two--banner-home-mobile",
      "flex-layout.col#text__one--banner-home-mobile"
    ]
  },
